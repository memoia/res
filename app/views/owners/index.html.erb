<h1>Listing Owners</h1>

<table>
  <thead>
    <tr>
      <th>Owner ID</th>
      <th>Name</th>
      <th>Desc</th>
      <th>Full address</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td><%= owner.owner_id %></td>
        <td><%= owner.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>